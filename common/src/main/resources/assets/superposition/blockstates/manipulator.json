{
  "multipart": [
    {
      "when": {
        "facing": "up"
      },
      "apply": {
        "model": "superposition:block/manipulator",
        "x" : -90
      }
    },
    {
      "when": {
        "facing": "down"
      },
      "apply": {
        "model": "superposition:block/manipulator",
        "x" : 90
      }
    },
    {
      "when": {
        "facing": "north"
      },
      "apply": {
        "model": "superposition:block/manipulator"
      }
    },
    {
      "when": {
        "facing": "south"
      },
      "apply": {
        "model": "superposition:block/manipulator",
        "y": 180
      }
    },
    {
      "when": {
        "facing": "west"
      },
      "apply": {
        "model": "superposition:block/manipulator",
        "y": 270
      }
    },
    {
      "when": {
        "facing": "east"
      },
      "apply": {
        "model": "superposition:block/manipulator",
        "y": 90
      }
    }
  ]
}